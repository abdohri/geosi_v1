
body {
margin:0px;
padding:0px;

}
.HautAthentif
{
    width: 100%;
    background-color: #F2F2F2;
    overflow: hidden;
    height: 96px;
}
.HautAthentif1
{
    width: 1000px;
    margin: auto;
    height: 96px;
    overflow: hidden;
}
.inputText
{
    border: 1px solid #E5E5E5;
    border-radius: 3px;
    width: 145px;
    height: 23px;
    padding-left: 5px;
}

.inputText1
{
    border: 1px solid #E5E5E5;
    border-radius: 3px;
    width: 130px;
    height: 23px;
    padding-left: 5px;
}

.boutonbleu
{
    width: 100px;
    border: 0px;
    padding: 5px 0;
    text-align: center;
    display: inline-block;
    font-size: 1.6em;
    border-radius: 7px;
    box-shadow: 0 0 1px rgba( 0, 0, 0, 0.2), 0 -1px 0 rgba( 255, 255, 255, 0.1);
    font-size: 0.85em;
    color: #FFF;
}
.boutonbleu, .boutonbleu:active
{
    background: #3C98CF;
    background: -webkit-linear-gradient( #3C98CF, #2D74A0);
    background: -moz-linear-gradient( #3C98CF, #2D74A0);
    background: -ms-linear-gradient( #3C98CF, #2D74A0);
    background: -o-linear-gradient( #3C98CF, #2D74A0);
    background: linear-gradient( #3C98CF, #2D74A0);
}
.boutonbleu:hover
{
    background: #3CA1D3;
    background: -webkit-linear-gradient( #3CA1D3, #3386AF);
    background: -moz-linear-gradient( #3CA1D3, #3386AF);
    background: -ms-linear-gradient( #3CA1D3, #3386AF);
    background: -o-linear-gradient( #3CA1D3, #3386AF);
    background: linear-gradient( #3CA1D3, #3386AF);
}
.boutonbleu:active
{
    box-shadow: 1px 1px 10px #1E516B inset, 0 1px 0 rgba( 255, 255, 255, 0.4);
}
#geoclear {
    clear:both;
}
